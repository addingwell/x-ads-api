const getAllEventData = require('getAllEventData');
const JSON = require('JSON');
const sendHttpRequest = require('sendHttpRequest');
const sha256Sync = require('sha256Sync');
const getRequestHeader = require('getRequestHeader');
const getType = require('getType');
const makeString = require('makeString');
const parseUrl = require('parseUrl');
const setCookie = require('setCookie');
const encodeUriComponent = require('encodeUriComponent');
const getCookieValues = require('getCookieValues');
const eventData = getAllEventData();
const twclid = getClickId();
const getRemoteAddress = require('getRemoteAddress');
const getTimestampMillis = require('getTimestampMillis');
const generateRandom = require('generateRandom');
const Math = require('Math');
const Object = require('Object');
const host = getRequestHeader('host');
const sendPixelFromBrowser = require('sendPixelFromBrowser');

if (twclid) {
  setCookie('twclid', twclid, {
    domain: 'auto',
    path: '/',
    samesite: 'Lax',
    secure: true,
    'max-age': 7776000, // 90 days
    httpOnly: false,
  });
}

let contents = getContentsFromItems();
let value = getValue();

let requestBody = {
  conversions: [{
    conversion_time: getConversionTime(),
    event_id: data.eventId,
    identifiers: getIdentifiers(),
    price_currency: eventData.currency ? eventData.currency : undefined,
    value: value,
    number_items: eventData.items ? eventData.items.length : 0,
    conversion_id: generateConversionId(),
    search_string: eventData.search_term ? eventData.searchTerm : undefined,
    contents: contents
}]};

if (data.serverEventDataList) {
  data.serverEventDataList.forEach(d => {
    if(d.name == "value") {
      requestBody.conversions[0][d.name] = makeDouble(d.value);
    } else {
      requestBody.conversions[0][d.name] = d.value;
    }
  });
  
}

if (requestBody.conversions[0].identifiers.length > 1) {
 
sendHttpRequest(
  'https://' + host + '/transformer/twitter/authorization-ads-api',
  (statusCode, headers, body) => {    
      if (statusCode >= 200 && statusCode < 300) {
        const bodyParsed = JSON.parse(body);
        const token = bodyParsed.value;
          
          sendHttpRequest('https://ads-api.twitter.com/12/measurement/conversions/' + data.pixelId,
          (statusCode, headers, body) => {
            if (statusCode >= 200 && statusCode < 300) {
                if (!!data.sendPixelRequest) {
                  sendPixelRequest();
                }
                data.gtmOnSuccess();
              } else {
                data.gtmOnFailure();
              }
            },
            {
              headers: {
                'Content-Type': 'application/json',
                Authorization: 'OAuth ' + token,
              },
              method: 'POST',
            },
            JSON.stringify(requestBody)
          );   
        
      } else {
        data.gtmOnFailure();
      }
  },
  {
    headers: {
      'Content-Type': 'application/json',
    },
    method: 'POST',
  },
  JSON.stringify({
 	"consumerKey": data.consumerKey,
 	"consumerSecret": data.consumerSecret,
 	"oauthToken": data.oauthToken,
 	"oauthTokenSecret": data.oauthTokenSecret,
 	"pixelId": data.pixelId,
 	"workspaceUuid": data.containerUuid
  })
);
    
} else {
  if (!!data.sendPixelRequest) {
    sendPixelRequest();
  }
  data.gtmOnSuccess();
}

function sendPixelRequest() {
  
  const eventId = data.eventId;
  const body = requestBody.conversions[0];
  
  let email;
  let phone_number;
  
  body.identifiers.forEach(identifier => {
    const key = Object.keys(identifier)[0];
    const value = identifier[key];

    if (key === "hashed_email") {
      email = value;
    } else if (key === "hashed_phone_number") {
      phone_number = value;
    }
  });
  
  let urlParams = [
        ['bci', '4'],
        ['eci', '4'],
        ['email_address', email],
        ['event', JSON.stringify({ value: body.value, currency: body.price_currency, conversion_id: body.conversion_id || generateConversionId(), contents: body.contents, search_string: body.search_string })],
        ['event_id', generateUUID()],
        ['integration', 'advertiser'],
        ['p_id', 'Twitter'],
        ['p_user_id', '0'],
        ['phone_number', phone_number],
        ['pl_id', generateUUID()],
        ['tw_acc_response', twclid ? 1 : undefined],
        ['tw_clid_src', twclid ? 1 : undefined],
        ['tw_document_href', eventData.page_location],
        ['tw_iframe_status', '0'],
        ['twclid', twclid],
        ['txn_id', enc(eventId)],
        ['type', 'javascript'],
        ['version', '2.3.31']
      ];
  
      sendPixelFromBrowser("https://analytics.twitter.com/1/i/adsct?" + urlParams.filter((v) => v[1]).map((v) => v[0] + '=' + encodeUriComponent(v[1].toString())).join('&'));
  
      sendPixelFromBrowser("https://t.co/1/i/adsct?" + urlParams.filter((v) => v[1]).map((v) => v[0] + '=' + encodeUriComponent(v[1].toString())).join('&'));

}

function enc(data) {
  data = data || '';
  return encodeUriComponent(data);
}

function generateUUID() {
  let len = 36;
  let chars = '0123456789abcdef'.split('');
  let uuid = '';

  for (var i = 0; i < len; i++) {
    if (i == 8 || i == 13 || i == 18 || i == 23) {
      uuid += '-';
    } else if (i == 14) {
      uuid += '4';
    } else {
      uuid += chars[generateRandom(0, chars.length - 1)];
    }
  }
  return uuid;
}

function getConversionTime() {
  
  let parsedDate = parseDate(getTimestampMillis());
  
  const replaceArray = {
    'YYYY': parsedDate.year,
    'MM': parsedDate.month,
    'DD': parsedDate.day,
    'hh': parsedDate.hours,
    'mm': parsedDate.minutes,
    'ss': parsedDate.seconds,
    'sss': parsedDate.ms,
  };

  let finalText = "%YYYY%-%MM%-%DD%T%hh%:%mm%:%ss%.%sss%Z";

  for (const entry of Object.entries(replaceArray)) {
    const replace = '%' + entry[0] + '%';
    while (finalText.indexOf(replace) !== -1) {
      finalText = finalText.replace('%' + entry[0] + '%', entry[1]);
    }
  }
  
  return finalText;  

}

function getIdentifiers() {
  
  let user_data = {}; 
  if (getType(eventData.user_data) === 'object') {
    user_data = eventData.user_data;
  }
  
  let tempIdentifiers = {};
  
  if(user_data.email_address) {
     tempIdentifiers.hashed_email = hashData(user_data.email_address);
   } else if(user_data.email) {
     tempIdentifiers.hashed_email = hashData(user_data.email);
   } else if(user_data.sha256_email_address) {
     tempIdentifiers.hashed_email = hashData(user_data.sha256_email_address);
   }
  
   if(user_data.phone_number) { 
     tempIdentifiers.hashed_phone_number = hashData(user_data.phone_number);
   } else if(user_data.sha256_phone_number) {
     tempIdentifiers.hashed_phone_number = hashData(user_data.sha256_phone_number);
   }

   if(eventData.user_agent) {
     tempIdentifiers.user_agent = eventData.user_agent;
   }
    
   let ip = eventData.ip_override || getRemoteAddress();
  
   if(ip) {
     tempIdentifiers.ip_address = ip;
   }
  
   const identifiers = [
     {
       twclid: twclid
     },
     {
       hashed_email: tempIdentifiers.hashed_email
     },
     {
       hashed_phone_number: tempIdentifiers.hashed_phone_number
     },
     {
       ip_address: tempIdentifiers.ip_address,
       user_agent: tempIdentifiers.user_agent
     }    
   ];
  
  
  
  if (data.identifiersList) {
    data.identifiersList.forEach(d => {
      if(d.name == "twclid") {
        identifiers[0][d.name] = d.value;
      } else if(d.name == "hashed_email"){
        identifiers[1][d.name] = hashData(d.value);
      } else if(d.name == "hashed_phone_number") {
        identifiers[2][d.name] = hashData(d.value);
      }
    });
  }
  
  for(let i = identifiers.length - 1; i >= 0; i--) {
    if(JSON.stringify(identifiers[i]) == "{}") {
      identifiers.splice(i, 1);
    }
  }
  
  return identifiers;  

}

function getContentsFromItems() {
  let items = eventData.items;
  let contents = [];
  
  if(items && getType(items) == "array" && items.length > 0) {
    items.forEach(item => {
      contents.push({
        content_id: item[getItemField('item_id')],
        content_name: item[getItemField('item_name')],
        content_type: item[getItemField('item_category')],
        content_price: makeDouble(item[getItemField('price')]),
        num_items: item[getItemField('quantity')],
        content_group_id: item[getItemField('item_list_id')]
      });
    });
  }
  
  return JSON.stringify(contents) == "[]" ? undefined : contents;
}

function makeDouble(value) {
    let str = makeString(value);
    if (str.indexOf(".") === -1) {
        return str + ".00";
    }
    let parts = str.split(".");
    return parts[0] + "." + (parts[1] + "00").substring(0, 2);
}

function getValue() {
  
  let value = eventData.value;
 
  if(value) {
    value = makeDouble(value);
  }
  
  return value;
  
}

function getItemField(defaultField) {
  let result = defaultField;

  const map = {
    item_id: 'item_id',
    price: 'price',
    quantity: 'quantity',
    item_list_id: 'item_list_id',
    item_name: 'item_name',
    item_category: 'item_category'
  };

  if (data.itemsDataList) {
    data.itemsDataList.forEach(d => {
      if (map[defaultField] === d.name) {
        result = d.field;
      }
    });
  }

  return result;
}

function getClickId() {
  return parseUrl(eventData.page_location || getRequestHeader('referer')).searchParams.twclid || getCookieValues('twclid')[0] || eventData.twclid;
}

function generateConversionId() {
  return getTimestampMillis() + '.' + generateRandom(1000000000, 2147483647);
}
  
function isAlreadyHashed(input){
  return input && (input.toString().match('^[A-Fa-f0-9]{64}$') != null);
}

function hashData(input){
  if(input == null || isAlreadyHashed(input)){
    return input;
  }

  return sha256Sync(input.toString().trim().toLowerCase(), {outputEncoding: 'hex'});
}

function padTwoNumbers(number) {
  if (number < 10) {
    return '0' + number;
  }

  return '' + number;
}

function padThreeNumbers(number) {
  if (number < 10) {
    return '00' + number;
  }
  
  if (number < 100) {
    return '0' + number;
  }

  return '' + number;
}

function parseDate(timestampMillis) {
  const timestampDays = Math.floor(timestampMillis / 60 / 60 / 24 / 1000);
  const days = timestampDays + 719468;

  const era = Math.floor((days >= 0 ? days : days - 146096) / 146097);
  const doe = days - era * 146097;
  const yoe = Math.floor((doe + (-Math.floor(doe / 1460)) + Math.floor(doe / 36524) - Math.floor(doe / 146096)) / 365);
  const y = yoe + era * 400;
  const doy = doe - (365 * yoe + Math.floor(yoe / 4) - Math.floor(yoe / 100));
  const mp = Math.floor((5 * doy + 2) / 153);
  const d = 1 + doy - Math.floor(((153 * mp) + 2) / 5);
  const m = mp < 10 ? mp + 3 : mp - 9;

  let timeLeft = (timestampMillis - (timestampDays * 60 * 60 * 24 * 1000)) / 1000;
  const hours = Math.floor(timeLeft / 60 / 60);
  timeLeft = timeLeft - hours * 60 * 60;
  const minutes = Math.floor(timeLeft / 60);
  timeLeft = timeLeft - minutes * 60;
  const seconds = Math.floor(timeLeft);
  timeLeft = timeLeft - seconds;

  return {
    year: y + (m <= 2),
    month: padTwoNumbers(m),
    day: padTwoNumbers(d),
    hours: padTwoNumbers(hours),
    minutes: padTwoNumbers(minutes),
    seconds: padTwoNumbers(seconds),
    ms: padThreeNumbers(Math.floor(timeLeft * 1000)),
  };
}


